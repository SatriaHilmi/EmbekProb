---
import astroLogo from "../assets/astro.svg";
import background from "../assets/background.svg";
import deadpool from "../assets/deadpoll.jpg";
import { Caraousel } from "../components/Caraousel";
---

<Caraousel
    client:load
    images={[
        {
            src: deadpool.src,
            title: "Deadpool",
            description: "This is Deadpool",
        },
        {
            src: background.src,
            title: "Background",
            description: "This is Background",
        },
        { src: astroLogo.src, title: "Astro", description: "This is Astro" },
    ]}
    autoSlide={true}
    slideInterval={6000}
/>
